__lua__
-- 웃 jump rein 웃
-- by tyler

function _init()
 make_player()
end

function _update()
 move_player()
end

function _draw()
 cls()
 draw_map()
 draw_player()
end

-->8
--map

function draw_map()
 
 map(0,48,0,0,128,128)
end
-->8
--player

function make_player()
 p={}
 p.x=32
 p.y=96
 p.sprite=1
 p.walktimer=2
 p.walkanimation=false
 p.iswalking=false
end

function draw_player()
 if(p.iswalking) then
  walk_animation()
 end
 spr(p.sprite,p.x,p.y,2,2)
end

function walk_animation()
 p.walktimer-=1
 if(p.walktimer < 0) then
  p.walktimer=2
  p.walkanimation=not p.walkanimation
 end
 if (p.walkanimation) then
  p.sprite=5
 else
  p.sprite=3
 end
end

function move_player()
 newx=p.x
 newy=p.y
 
 if (btn(⬅️)) then
  newx-=2
  p.iswalking=true
 elseif (btn(➡️)) then
  newx+=2
  p.iswalking=true
 elseif (btn(⬆️)) then
  newy-=1
 elseif (btn(⬇️)) then
  newy+=1
 else
  p.sprite=1
  p.iswalking=false
 end
 
 
  p.x=newx
  p.y=newy
end
